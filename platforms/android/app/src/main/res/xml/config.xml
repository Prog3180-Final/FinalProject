<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.moviedatabase" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="CertificatesPlugin">
        <param name="android-package" value="de.martinreinhardt.cordova.plugins.CertificatesPlugin" />
    </feature>
    <name>Movie database</name>
    <icon density="mdpi" height="57" platform="android" src="res/icon/android/icon.png" witdh="57" />
    <description>
        Final project - Movie database by Rawa Jalal and Steven Ye
    </description>
    <author>
        Rawa Jalal and Steven Ye
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="permissions" value="none" />
</widget>
